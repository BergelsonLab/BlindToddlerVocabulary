@article{rader2012,
  title = {Caregivers' Gestures Direct Infant Attention during Early Word Learning: The Importance of Dynamic Synchrony},
  shorttitle = {Caregivers' Gestures Direct Infant Attention during Early Word Learning},
  author = {Rader, Nancy de Villiers and {Zukow-Goldring}, Patricia},
