  author = {Bourne, Rupert R A and Adelson, Jaimie and Flaxman, Seth and Briant, Paul and Bottone, Michele and Vos, Theo and Naidoo, Kovin and Braithwaite, Tasanee and Cicinelli, Maria and Jonas, Jost and Limburg, Hans and Resnikoff, Serge and Silvester, Alex and Nangia, Vinay and Taylor, Hugh R},
  year = {2020},
