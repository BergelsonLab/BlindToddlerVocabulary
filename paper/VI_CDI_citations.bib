@article{bottini2022,
  title = {The Concreteness Advantage in Lexical Decision Does Not Depend on Perceptual Simulations},
  author = {Bottini, Roberto and Morucci, Piermatteo and D'Urso, Anna and Collignon, Olivier and Crepaldi, Davide},
